<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ntsan.examplejavaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExampleJavaApp">
        <activity android:name=".AudioPlayerActivity"></activity>
        <activity android:name=".HigherOrLowerActivity" />
        <activity android:name=".VideoActivity" />
        <activity android:name=".AudioActivityKt" />
        <activity android:name=".AudioActivity" />
        <activity android:name=".BackgroundActivity" />
        <activity android:name=".SlowFadingActivity" />
        <activity android:name=".GifActivity" />
        <activity android:name=".ButtonActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>